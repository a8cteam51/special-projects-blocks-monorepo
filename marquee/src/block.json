{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "wpcomsp/marquee",
	"version": "0.1.0",
	"title": "Marquee",
	"category": "widgets",
	"icon": "smiley",
	"description": "A Marquee block that scrolls content horizontally.",
	"example": {},
	"supports": {
		"interactivity": true,
		"anchor": true,
		"html": false
	},
	"attributes": {
		"tagName": {
			"type": "string",
			"default": "div"
		}
	},
	"textdomain": "marquee",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScriptModule": "file:./view.js"
}
