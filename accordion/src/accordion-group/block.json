{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "wpsp/accordion",
	"version": "0.1.0",
	"title": "Accordion",
	"category": "design",
	"icon": "list",
	"description": "Container block for accordion items.",
	"example": {},
	"supports": {
		"html": false,
		"align": true,
		"spacing": {
			"padding": true,
			"margin": true,
			"blockGap": true
		},
		"layout": true,
		"interactivity": true
	},
	"allowedBlocks": [ "wpsp/accordion-item" ],
	"textdomain": "accordion",
	"editorScript": "file:./index.js",
	"viewScriptModule": "file:./view.js",
	"render": "file:./render.php"
}
