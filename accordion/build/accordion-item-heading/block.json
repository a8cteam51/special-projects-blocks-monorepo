{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wpsp/accordion-item-trigger",
  "version": "0.1.0",
  "title": "Accordion Item Trigger",
  "category": "design",
  "description": "Accordion item trigger ",
  "example": {},
  "parent": [
    "wpsp/accordion-item"
  ],
  "supports": {
    "html": false,
    "interactivity": true,
    "color": {
      "background": true,
      "text": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "textAlign": true,
      "__experimentalFontFamily": true
    }
  },
  "attributes": {
    "title": {
      "type": "rich-text",
      "source": "rich-text",
      "selector": "span"
    },
    "level": {
      "type": "number",
      "default": 3
    },
    "textAlign": {
      "type": "string"
    }
  },
  "textdomain": "accordion-heading",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}